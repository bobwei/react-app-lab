{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import"
  ],
  "extends": "airbnb",
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-console": 0,
    "no-alert": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "react/prop-types": [2, {"ignore": ["className", "children", "id", "onClick", "style", "dispatch", "params", "location"]}]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "extensions": ["*", ".js", ".jsx"]
      }
    }
  }
}
